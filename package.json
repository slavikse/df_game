{
  "name": "martyr",
  "description": "Мученик - машина, а не человек! Современный стек технологий для сборки веб проектов",
  "version": "3.12.4",
  "scripts": {
    "clean": "rm -rf public temp && mkdir public temp",
    "dev": "gulp",
    "prod": "NODE_ENV=production BABEL_ENV=production gulp",
    "tunnel": "NODE_ENV=tunnel gulp bs",
    "fb-login": "firebase login",
    "fb-init": "firebase init",
    "fb-deploy": "firebase deploy",
    "deploy": "time npm run prod && npm run fb-deploy",
    "npm-clean": "npm prune && npm dedupe",
    "line": "find source \\( -name '*.html' -or -name '*.css' -or -name '*.js'  \\) -type f -print0 | xargs -0 cat | wc -l",
    "critical": "gulp critical"
  },
